{{> navbar}}
<div class="container">
   
<div class="row mt-5">
  <div class="col-6"><h5>CORE about</h5></div>
  <div class="col-6 justify-content-end">
    <a href="/about/addAbout" type="button" class="btn btn-primary align-self-center mb-3">Add About</a>
  </div>
</div>
<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Description</th>
      <th>Action</th>

    </tr>
  </thead>
  <tbody>
    {{#each rows}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.description}}</td>

     

      <td class="text-end">
          <a href="/about/editAbout/{{this.id}}" type="button" class="btn btn-light btn-small m-1"> <i class="fa fa-pencil">Edit</i></a>
          <a href="/about/deleteAbout/{{this.id}}" type="button" class="btn btn-light btn-small m-1"> <i class="fa fa-trash">Delete</i></a>
      
      </td>
    </tr>
    {{/each}}

    
  </tbody>
</table>
</div>